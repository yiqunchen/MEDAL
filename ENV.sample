# Copy to .env and fill in your keys.
# NEVER commit .env to git!

# OpenAI API (required for GPT-4o, GPT-5, GPT-4o-mini evaluations)
OPENAI_API_KEY=sk-...

# OpenRouter API (required for Claude Sonnet 4.5, DeepSeek evaluations)
OPENROUTER_API_KEY=sk-or-v1-...
OPENROUTER_HTTP_REFERER=https://github.com
OPENROUTER_X_TITLE=MEDAL Evaluation

# Anthropic API (optional, for direct Claude API access)
ANTHROPIC_API_KEY=sk-ant-...

# Entrez/PubMed API (optional, for fetching PubMed abstracts)
ENTREZ_API_KEY=

# Cochrane scraping (optional, for downloading Cochrane reviews)
COCHRANE_USERNAME=
COCHRANE_PASSWORD=

